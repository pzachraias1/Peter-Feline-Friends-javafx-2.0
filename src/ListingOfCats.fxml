<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="catPreview" onMousePressed="#click" prefHeight="150.0" prefWidth="130.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="catListingController">
    <stylesheets>
        <URL value="@pff.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="-9.0" prefHeight="150.0" prefWidth="130.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="previewCatName" text="cat name">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <ImageView fx:id="previewCatImage" fitHeight="69.0" fitWidth="121.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </VBox>
   </children>
</AnchorPane>
